@@BEGIN:	VERSES
@ABOUTIT:	[TIRSI E LICORI]
@ABOUTEN:	[THYRSIS AND LYCORIS]
@EDITOR:	Solerti, Angelo
@EDITION_TITLE: Le Rime di Torquato Tasso
